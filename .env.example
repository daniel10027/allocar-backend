SECURITY_PASSWORD_SALT=change_me_salt

# Postgres
DB_HOST=postgres
DB_PORT=5432
DB_NAME=allocar
DB_USER=allocar
DB_PASSWORD=allocar

# Redis
REDIS_URL=redis://redis:6379/0

# Celery
CELERY_BROKER_URL=redis://redis:6379/1
